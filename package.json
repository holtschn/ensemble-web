{
  "name": "ensemble-web",
  "version": "0.0.0",
  "description": "Website for a Brass Ensemble with React, Next.js and Payload)",
  "license": "MIT",
  "type": "module",
  "scripts": {
    "build": "next build",
    "dev": "next dev",
    "devsafe": "rm -rf .next && next dev",
    "generate:importmap": "payload generate:importmap",
    "generate:types": "payload generate:types",
    "generate:graphql": "payload-graphql generate:schema",
    "format": "prettier --write \"**/*.{ts,tsx,json,js,jsx,html,yml,md}\" \"!**/build/**\"",
    "lint": "next lint",
    "payload": "payload",
    "start": "next start",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:ci": "jest --ci --coverage --watchAll=false",
    "test:ndb": "jest src/next/ndb",
    "test:ndb:watch": "jest src/next/ndb --watch",
    "test:utils": "jest src/next/ndb/utils",
    "test:verbose": "jest --verbose"
  },
  "dependencies": {
    "@lexical/clipboard": "^0.35.0",
    "@lexical/file": "^0.35.0",
    "@lexical/html": "^0.35.0",
    "@lexical/link": "^0.35.0",
    "@lexical/list": "^0.35.0",
    "@lexical/markdown": "^0.35.0",
    "@lexical/overflow": "^0.35.0",
    "@lexical/plain-text": "^0.35.0",
    "@lexical/react": "^0.35.0",
    "@lexical/rich-text": "^0.35.0",
    "@lexical/selection": "^0.35.0",
    "@lexical/table": "^0.35.0",
    "@lexical/utils": "^0.35.0",
    "@payloadcms/db-postgres": "^3.59.1",
    "@payloadcms/email-nodemailer": "^3.59.1",
    "@payloadcms/live-preview-react": "^3.59.1",
    "@payloadcms/next": "^3.59.1",
    "@payloadcms/plugin-seo": "^3.59.1",
    "@payloadcms/richtext-lexical": "^3.59.1",
    "@payloadcms/storage-s3": "^3.59.1",
    "@payloadcms/storage-vercel-blob": "^3.59.1",
    "@vercel/analytics": "^1.5.0",
    "@vercel/speed-insights": "^1.2.0",
    "escape-html": "^1.0.3",
    "next": "^15.5.4",
    "nodemailer": "^7.0.9",
    "payload": "^3.59.1",
    "react": "19.2.0",
    "react-dom": "19.2.0",
    "sonner": "^2.0.7"
  },
  "devDependencies": {
    "@tailwindcss/postcss": "^4.1.14",
    "@testing-library/dom": "^10.4.1",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/escape-html": "^1.0.4",
    "@types/jest": "^30.0.0",
    "@types/node": "^24.6.2",
    "@types/nodemailer": "^7.0.2",
    "@types/react": "19.2.0",
    "@types/react-dom": "19.2.0",
    "esbuild": ">=0.25.0",
    "eslint": "^9.36.0",
    "eslint-config-next": "^15.5.4",
    "jest": "^30.2.0",
    "jest-environment-jsdom": "^30.2.0",
    "msw": "^2.11.3",
    "postcss": "^8",
    "tailwindcss": "^4.1.14",
    "ts-jest": "^29.4.4",
    "typescript": "^5.9.3"
  },
  "overrides": {
    "nodemailer": "^7.0.9",
    "esbuild": ">=0.25.0"
  }
